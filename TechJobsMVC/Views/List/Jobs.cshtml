using System.Collections.Generic;
<div class="container body-content">

    <h1>@ViewBag.title</h1>

    @foreach (Job item in ViewBag.jobs)
    {
<table class="job-listing">
    <tr>
        <td>Id</td>
        <td>@item.Id</td>
    </tr>
    <tr>
        <td>Name</td>
        <td>@item.Name</td>
    <tr>
        <td>Employer</td>
        <td>@item.Employer</td>
    <tr>
        <td>Location</td>
        <td>@item.Location</td>
    </tr>
    <tr>
        <td>Position Type</td>
        <td>@item.PositionType</td>
    </tr>
    <tr>
        <td>Skill</td>
        <td>@item.CoreCompetency</td></tr>



</table>
    }



    


</div>